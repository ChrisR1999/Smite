<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.arturo.linearyscrollbar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/perdongato"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/perdongato"
        android:supportsRtl="true"
        android:theme="@style/SplashTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:icon,android:roundIcon,android:theme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Buscar" />
        <activity android:name=".Smiteguru" />
        <activity android:name=".PaginaWeb" />
        <activity android:name=".ItemRandom" />
        <activity android:name=".BuildMaker" />
        <activity android:name=".TodosLosItems" />
        <activity android:name=".DatosDeLosItems" />
    </application>
</manifest>